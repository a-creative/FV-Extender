{
  "name": "FV Extender",
  "version": "3.0.12",
  "description": "Extends FarmVille with new features",
  "background_page": "html/background.html",
  "content_scripts": [
    {
      "matches": [
		"http://www.farmville.com/*",
		"http://www.facebook.com/reqs.php",
		"https://www.facebook.com/reqs.php",
		"http://apps.facebook.com/onthefarm*"
	  ],
      "js": ["js/jquery.js","js/shared.js","js/main_content_script.js","js/start_content_script.js"],
      "css" : ["css/style.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
	"https://apps.facebook.com/onthefarm*",
	"http://apps.facebook.com/onthefarm*",
	"http://www.facebook.com/ajax/reqs.php?__a=1",
	"http://www.facebook.com/ajax/reqs.php",
	"http://*.farmville.com/*",
	"http://a-creative.github.com/*"
  ],
  "icons": {
  	"16" : "graphics/chrome_16.jpg",
  	"32" : "graphics/chrome_32.jpg",
  	"48" : "graphics/chrome_48.jpg",
  	"128" : "graphics/chrome_128.jpg"
  }
}